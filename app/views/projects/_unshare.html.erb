<table class="table table-hover">
  <thead>
    <tr>
      <th>id</th>
      <th>user</th>
      <th>time</th>
      <th>comment</th>
    </tr>
  </thead>
  <tbody>
    <% @current_project.each do |episode| %>
    <tr>
      <td><%= episode.id %></td>
      <td><%= episode.user.name %></td>
      <td><%= episode.created_at %></td>
      <td><%= episode.comment %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "Share", share_path,
            class: "btn btn-large btn-primary" %>

